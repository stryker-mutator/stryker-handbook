process:MutationTestProcess[a] "MutationTestProcess"
mutant-transpiler:MutantTranspiler[a]
/facade:TranspilerFacade[a]
pool:SandboxPool[a]
/sandbox:Sandbox[a]
reporter:Reporter[a]

process:mutant-transpiler.Initialize(inputFiles)
[c:child_process]
mutant-transpiler:facade.new
mutant-transpiler:facade.transpile(files)&
[/c]
process:mutantStream=mutant-transpiler.TranspileMutants(mutants)
[c:child_process]
[c:foreach_mutant]
mutant-transpiler:facade.transpileMutant(mutant)
[/c]
[/c]
process:sandboxxStream=pool.streamSandboxes()
[c:foreach_cpu]
pool:sandbox.new
[/c]
[c:foreach_transpiled_mutant]
process:runResult=sandbox.runMutant(transpiledMutant)
[c:RunTest]
sandbox:sandbox.runTest
[/c]
process:reporter.onMutantResult(mutantResult)
[/c]
process:reporter.onAllMutantsTested(mutantResults)